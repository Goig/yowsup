    <xs:simpleType name="media_upload_type">
        <xs:restriction base="xs:string">
            <xs:enumeration value="image" />
            <xs:enumeration value="video" />
            <xs:enumeration value="audio" />
        </xs:restriction>
    </xs:simpleType>

</xs:schema>